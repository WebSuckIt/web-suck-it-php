{
    "name": "websuckit/websuckit-php",
    "description": "Websuckit PHP library",
    "homepage": "https://github.com/WebSuckIt/web-suck-it-php",
    "type": "library",
    "license": "MIT",
    "keywords": ["websocket", "communication", "http", "websuckit", "realtime", "wss"],
    "autoload": {
        "psr-4": {
            "Websuckit\\WebsuckitPhp\\": "src"
        },
        "exclude-from-classmap": ["tests/", "example/"]
    },
    "autoload-dev": {
        "psr-4": { "Websuckit\\WebsuckitPhp\\Tests\\": "tests" }
    },
    "authors": [
        {
            "name": "Wisdom Praise",
            "email": "wisdompraise968@gmail.com",
            "homepage": "https://github.com/wizzywit",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "phpseclib/phpseclib": "^3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "pestphp/pest": "^2.24",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
